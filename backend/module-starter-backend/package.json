{
  "name": "@pardjs/module-starter-backend",
  "version": "0.0.1",
  "description": "backend",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prepublish": "npm run build",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.5",
    "@nestjs/core": "^6.0.5",
    "@pardjs/common": "^0.0.4",
    "fastify-formbody": "^3.1.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.5",
    "@types/express": "^4.0.39",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/supertest": "^2.0.4",
    "jest": "^24.7.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.15.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
